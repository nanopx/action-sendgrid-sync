{
  "name": "sendgrid-sync",
  "version": "0.5.2",
  "description": "GitHub action to sync handlebars template with sendgrid",
  "main": "lib/syncSendgrid.js",
  "bin": "dist/cli/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "clean": "rimraf ./lib && rimraf ./dist",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build lib/main.js -o dist/ --source-map --license licenses.txt",
    "package-cli": "ncc build lib/cli.js -o dist/cli --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run clean && npm run build && npm run format && npm run lint && npm run package && npm run package-cli && npm test",
    "prepare": "npm run all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanopx/action-sendgrid-sync.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "meow": "^9.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.28.4",
    "eslint": "^7.13.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
